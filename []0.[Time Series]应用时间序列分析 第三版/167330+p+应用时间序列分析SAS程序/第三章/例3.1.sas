data a;
y1=0;
y2=0;
y3=0;
a=1;
do t=-10 to 100;
e=rannor(12345);
y1=a*t+e;
y2=y2+e;
y3=y3+a+e;
if t>0 then output ;
end;
data a;
set a;
keep t y1 y2 y3;
proc gplot;
plot y1*t y2*t y3*t ;
symbol c=red i=jion v=none;
data a;
set a;
dify1=dif(y1);
proc arima data=a;
identify var=dify1;
run;
proc reg data=a;
model y1=t/noint;
output out=out r=r;
proc gplot data=out;
plot r*t;
proc arima data=out;
identify var=r;
run;
